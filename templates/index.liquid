<div class="banner">
     <ul>
         {% if settings.slide_1 != false or settings.slide_2 != false or settings.slide_3 != false or settings.slide_4 != false %}
         <!-- Begin slider -->
                 {% for i in (1..4) %}
                   {% capture show_slide %}slide_{{ i }}{% endcapture %}
                   {% capture image %}slideshow_{{ i }}.jpg{% endcapture %}
                   {% capture link %}slide_{{ i }}_link{% endcapture %}
                   {% capture alt %}slide_{{ i }}_alt{% endcapture %}
                   {% if settings[show_slide] %}
                   <li>
                     <a href="{{ settings[link] }}">
                       <img src="{{ image | asset_url }}" alt="{{ settings[alt] | escape }}" />
                       <div class="one">
                        <div class="emblem"></div>
                        <div class="tag"></div>
                        <a href="" class="prod">view products</a></div>
                       <div class="two">goodbye</div>
                       <div class="three">so long</div>
                       
                     </a>
                   </li>
                   {% endif %}
                 {% endfor %}
              {% endif %}
     </ul>
     <a href="#" class="find"><img src="https://cdn.shopify.com/s/files/1/0677/1369/files/find.png?10464" alt=""></a>
     <a href="#" class="find become"><img src="https://cdn.shopify.com/s/files/1/0677/1369/files/become.png?10464" alt=""></a>
     <a href="#" class="find customize"><img src="https://cdn.shopify.com/s/files/1/0677/1369/files/customize.png?10464" alt=""></a>
 </div> 


<div class="container-fluid social" >
  <a href="#" class="fb"></a>
  <a href="#" class="tw"></a>
  <a href="#" class="gg"></a>
  <a href="#" class="yt"></a>
</div>

<div class="container-fluid" style="background: #e6e6e6; padding: 20px 0 18px; border-top: 1px solid white; border-bottom: 1px solid white;">
  <div class="container no-pad articlas">
    <div class="col-md-4 col-sm-4">
      <a id="transform"><span class="black"></span><div class="text">transform: <br> stick to protocol</div></a>
    </div>
    <div class="col-md-4 col-sm-4">
      <a href="/collections/featured" id="featured"><span class="black"></span><div class="text">featured <br> products</div></a>
    </div>
    <div class="col-md-4 col-sm-4">
      <a id="join"><span class="black"></span><div class="text">join our <br> movement</div></a>
    </div>
  </div>
</div>  
<!-- start featured product -->
 <div class="container-fluid" style="border-top: 1px solid rgba(128, 128, 128, 0.46);">
  <div class="container" style="margin-top: 50px;">
    <div class="featured-main"><img src="https://cdn.shopify.com/s/files/1/0677/1369/files/featured.png?6979" alt=""></div>
    {% if settings.show_fp_images %}
    <!-- Begin promo images -->
    <div class="row" id="fp-images" style="margin-bottom:25px">
      {% for i in (1..3) %}
      {% capture fp_image %}fp_image_{{ i }}.jpg{% endcapture %}
      {% capture fp_link %}fp_image_{{ i }}_link{% endcapture %}
      {% capture fp_alt %}fp_image_{{ i }}_alt{% endcapture %}
      <div class="span4">
        <a href="{{ settings[fp_link] }}">
          <img src="{{ fp_image | asset_url }}" alt="{{ settings[fp_alt] | escape }}" />
        </a>
      </div>
      {% endfor %}
    </div>
    <!-- End promo images -->
    {% endif %}

    {% if settings.homepage != blank %}
    <!-- Begin intro -->
    <div class="span12 p20">
      <div class="intro clearfix">
        <h2>{{ pages[settings.homepage].title }}</h2>
        <div>{{ pages[settings.homepage].content }}</div>
      </div>
    </div>
    <!-- End intro -->
    {% endif %}
          
    <div class="products" style="border-top: none;">
      {% for product in collections[settings.frontpage_collection].products %}
      <div class="col-md-3 each no-pad">{% include 'front-product-loop' with settings.frontpage_collection %}</div>
      {% endfor %}
    </div>

    {% if settings.show_collections and settings.frontpage_collections != '' and settings.frontpage_collections != 'main-menu' and linklists[settings.frontpage_collections].links.size > 0 %}
    <div class="products">
      {% for link in linklists[settings.frontpage_collections].links %}
      {% if link.type == 'collection_link' %}
        {% assign collection = link.object %}
        {% include 'collection-loop' %}
      {% endif %}
      {% endfor %}
    </div>

    {% endif %}
  </div>
</div> 

