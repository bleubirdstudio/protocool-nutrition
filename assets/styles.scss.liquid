body, html{ height: 100%; display: inline-block; width: 100%;}
body{ overflow-x: hidden;}
.container-fluid, .container{
  min-width: 1200px;
}
h1{
  font-family: 'Oswald', sans-serif !important;
  text-transform: uppercase;
}
.no-pad{ padding: 0 !important;}
#wrapper {
  min-height:60%;
  position:relative;
}
.toolbar {
  background: rgba(204, 204, 204, 0.55);
  background-size: 10px;
  height: 50px;
}
.toolbar ul{
  display: inline-block;
  margin-top: 10px;
  &:first-of-type{
    padding-left: 0;
    float: left;
    li{
      font-family: 'Oswald', sans-serif;
      font-weight: 700;
      font-size: 18px;
      color: #191919;
      text-transform: uppercase;
      letter-spacing: 1px;
      a{
        color: #bd3a25;
        opacity: .75;
        &:hover{
          opacity: 1;
        }
      }
    }
  }
  &:last-of-type{
    float: right;
    li{
      margin: 5px 10px 0 10px;
      &:last-of-type{
        margin-right: 0;
      }
      a{
        text-transform: uppercase;
        font-size: 13px;
        font-weight: 700;
        font-family: 'Oswald', sans-serif;
        font-weight: 700;
        color: #333333;
        letter-spacing: 1px;
        opacity: .85;
        &:hover{
          opacity: 1;
        }
      }
    }
  }
  li{
    display: inline-block;
  }
}

.navigational{
  &#sticker{
    width: 100%;
  }
  background-image: -webkit-gradient(
    linear,
    left top,
    left bottom,
    color-stop(0, #5E5E5E),
    color-stop(1, #0D0D0D)
  );
  background-image: -o-linear-gradient(bottom, #5E5E5E 0%, #0D0D0D 100%);
  background-image: -moz-linear-gradient(bottom, #5E5E5E 0%, #0D0D0D 100%);
  background-image: -webkit-linear-gradient(bottom, #5E5E5E 0%, #0D0D0D 100%);
  background-image: -ms-linear-gradient(bottom, #5E5E5E 0%, #0D0D0D 100%);
  background-image: linear-gradient(to bottom, #5E5E5E 0%, #0D0D0D 100%);
  height: 109px;
  &.muscle{
    border-top: 1px solid rgba(255,255,255,.5);
    box-shadow: 0px 5px 25px -3px rgba(0,0,0,0.5);
    height: 110px;
    position: relative;
    z-index: 20;
  }
  img{
    width: 100%;
  }
  ul{
    margin-bottom: 0;
    padding: 0;
    margin-left: 30px;
    height: inherit;
    border-right: 1px solid rgba(255, 255, 255, .25);
    border-left: 2px solid rgba(0, 0, 0,.25);
    li{
      display: inline-block;
      font-family: 'Oswald', sans-serif;
      font-weight: 700;
      text-transform: uppercase;
      vertical-align: middle;
      position: relative;
      border-left: 1px solid rgba(255, 255, 255, .25);
      border-right: 1px solid rgba(0, 0, 0,.25);
      margin: 0 -1.75px;
      height: inherit;
      /*&:first-of-type{
        border-left: none;
      }
      &:last-of-type{
        border-right: none;
      }*/
      
      a:after{
        content: "\e114";
        font-family: 'Glyphicons Halflings';
        font-size: 12px;
        margin-left: 10px;
      }
      a{
        color: #e6e6e6;
        font-size: 16px;
        padding: 42px 13px 38px 13px;
        display: block;
        position: relative;
        letter-spacing: 1px;
        height: inherit;
        -webkit-transition: .15s;
        -moz-transition: .15s;
        -ms-transition: .15s;
        -o-transition: .15s;
        &:hover{
          color: #bd3a25;
          text-decoration: none; 
          background: #333333;
        }
      }
      &.hover{
        a{
          color: #bd3a25;
          text-decoration: none; 
          background: #333333;
        }
        .dropdown{
          a{
            font-size: 13px;
            padding-top: 10px;
            padding-bottom: 10px;
            color: white;
            font-weight: bold;
            letter-spacing: 0;
            font-family: helvetica, sans-serif, arial;
            border-radius: 0 0 10px 10px;
            &:after{
              display: none;
            }
          }
        }
      }
    }
    &.dropdown{
      width: 125%;
      height: auto;
      max-height: 600px;
      overflow: auto;
      padding: 0px;
      display: none;
      opacity: 0;
      margin: 0px;
      position: absolute;
      background: #333333;
      border: none;
      border-radius: 0 0 10px 10px;
      &::-webkit-scrollbar {
          width: 20px;
      }
       
      &::-webkit-scrollbar-track {
          -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
          border-radius: 5px;
          background: #666666;
      }
       
      &::-webkit-scrollbar-thumb {
          border-radius: 5px;
          background: #333333;
          -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5); 
      }
      li{
        position: relative;
        width: 100%;
        margin: 0;
        padding: 0;
        border: 0;
        border: none;
        border-bottom: 1px solid rgba(255,255,255,.25);
        &:last-of-type{
          border-bottom: none;
          border-radius: 0 0 10px 10px !important;
        }
        a{
          font-size: 13px;
          padding-top: 10px;
          padding-bottom: 10px;
          color: white;
          font-weight: bold;
          letter-spacing: 0;
          font-family: helvetica, sans-serif, arial;
          &:hover{
            color: #bd3a25;
            text-decoration: none; 
            background: #333333;
          }
          &:after{
            display: none;
          }
        }
      }
    }
  }
  h1{
    margin-bottom: 20px;
  }
  .logo{
    display: inline-block;
    width: 22%;
    float: left;
    margin-top: 4px;
  }
  .main{
    display: inline-block;
    float: left;
  }
}
.search-field{
  display: inline-block;
  border-right: 1px solid rgba(255, 255, 255, .25);
  .got{
    cursor: pointer;
    &:after{
      content: "\e003";
      font-family: 'Glyphicons Halflings';
      font-size: 20px;
      display: inline-block;
      padding: 40px 25px;
      color: #e6e6e6;
      border-right: 1px solid rgba(0, 0, 0,.25);
    }
    &:hover, &.current{
      &:after{
        color: #bd3a25;
        background: #333333;
      }
    }
  }
  form.search{
    display: inline-block;
    position: absolute;
    height: 0px;
    width: 100%;
    background: rgba(0,0,0,.95);
    padding: 0;
    margin: 0;
    left: 0;
    overflow: hidden;
    -webkit-transition: .15s;
    -moz-transition: .15s;
    -ms-transition: .15s;
    -o-transition: .15s;
    &.open{
      height: 100px;
    }
    input[type="image"]{
      width: 80px;
      padding: 32px 35px 30px 15px;
      margin-left: 20px;
      outline: none;
    }
    .search_box{
      width: 90%;
      height: inherit;
      float: right;
      background: transparent;
      border: none;
      border-left: 1px solid rgba(255,255,255,.35);
      padding-left: 30px;
      font-size: 50px;
      text-transform: uppercase;
      color: #e6e6e6;
      letter-spacing: 2px;
      outline: none;
    }
    ::-webkit-input-placeholder { color: #4c4c4c; }
    :-moz-placeholder { color: #4c4c4c; }
    ::-moz-placeholder { color: #4c4c4c; }
    :-ms-input-placeholder { color: #4c4c4c; }
  }
}


.mycoach{
  width: 400px;
  display: inline-block;
  padding: 20px;
  border-radius: 10px;
  background: rgba(169, 169, 169, 0.15);
  border: solid rgba(137, 136, 136, 0.35);
  label{
    font-size: 12px;
    margin-left: 10px;
