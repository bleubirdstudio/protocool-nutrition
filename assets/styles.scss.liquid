  }
  .explain{
    font-size: 11px;
    color: #b00f22;
    font-weight: bold; 
    margin: 10px;
  }
}
.question{
  width: 400px;
  text-align: center;
  font-size: 12px;
  font-weight: bold;
  margin-top: 20px; 
  a{
    text-decoration: none;
    color: #b00f22;
  }
}
.customer-tools{  
  display: inline-block;
  float: right;
  text-transform: uppercase;
  font-weight: bold;
  font-size: 15px;
  margin-top: 35px;
  color: #e6e6e6;
  text-align: right;
  a{
    color: #e6e6e6;
  }
  #cart-target, #checkout-link{
    display: inline-block;
  }
  #cart-target{
    position: relative;
    &:hover{
      .full-cart{
        display: inline-block;
      }
    }
    .full-cart{
      display: none;
      position: absolute;
      right: 0;
      top: 40px;
      width: 300px;
      height: auto;
      background: white;
      color: black;
      text-align: left;
      padding: 15px;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
      &:after{
        content: '';
        display: inline-block;
        border-left: 10px solid transparent;
        border-right: 10px solid transparent;
        border-bottom: 8px solid white;
        position: absolute;
        top: -8px;
        right: 5px;
      }
      .items-all{
        display: inline-block;
        width: 100%;
        height: auto;
        max-height: 300px;
        overflow: auto;
      }
      .items-in-cart{
        display: block;
        float: left;
        width: 100%;
        img{
        float: left;
        width: 100px;
        }
        .title{
          margin-top: 10px;
          font-family: helvetica, sans-serif, arial;
          font-weight: bold;
          font-size: 15px;
          color: #a32c2c;
          padding-bottom: 10px;
          margin-bottom: 10px;
          border-bottom: 1px solid #e6e6e6;
        }
        .price, .quantity{
          font-family: helvetica, sans-serif, arial;
          font-weight: bold;
          font-size: 13px;
          color: #666666;
        }
      }
      .top{
        font-family: helvetica;
        font-size: 12px;
        font-family: helvetica;
        font-size: 12px;
        padding-bottom: 10px;
        margin-bottom: 10px;
        border-bottom: 1px solid #808080;
        .right{
          float: right;
        }
      }
    }
    .cart{
      opacity: 1;
      position: relative;
      padding: 44px 0;
      &:before{
        content: "\e116";
        font-family: 'Glyphicons Halflings';
        font-size: 15px;
        display: inline-block;
        color: #e6e6e6;
        margin-right: 10px;
      }
      .empty{
        font-size: 12px;
        opacity: .75;
        margin: 0 5px 0 -10px; 
      }
      .bag{
        margin-left: 10px;
        -webkit-transition: .25s;
        -moz-transition: .25s;
        -ms-transition: .25s;
        -o-transition: .25s;
        position: relative;
        margin-bottom: 10px;
        display: inline-block;
        margin-right: 10px;
        .number{
          position: relative;
          z-index: 2
        }
        .circle{
          width: 30px;
          height: 30px;
          border-radius: 50%;
          border: 1px solid white;
          display: inline-block;
          position: absolute;
          z-index: 1;
          top: -5px;
          right: -11px;
          -webkit-transition: .25s;
          -moz-transition: .25s;
          -ms-transition: .25s;
          -o-transition: .25s;
        }
      }
      &:hover{
        color: white;
        text-decoration: none;
        .bag{
          color: white !important;
          .circle{
            background: #bd3a25;
            -webkit-transform: scale(1.25);
            -moz-transform: scale(1.25);
            -o-transform: scale(1.25);
            -ms-transform: scale(1.25);
          }
        }
      }
      &:hover{
        opacity: 1;
        .full-cart{
          display: inline-block;
        }
      }
    }
  }
  .toolbar-customer{
    display: inline-block;
    font-size: 12px;
    a{
      opacity: .75;
      color: white;
      margin-right: 10px;
      &:hover{
        text-decoration: none;
      }
    }
    #customer_register_link, .or{
      display: none;
    }
  }
}

.featured-main{
  img{
    display: block;
    margin: 10px auto 50px; 
  }
}
.outerbanner{
  width: 100%;
  display: block;
  background: black;
  .find {
    width: 210px;
    height: 75px;
    display: inline-block;
    position: absolute;
    top: 30px;
    left: 0;
    background-color: white;
    border-radius: 0 10px 10px 0;
    box-shadow: 5px 0 30px -5px black;
    padding-right: 10px;
    opacity: .75;
    -webkit-transition: .25s;
    -moz-transition: .25s;
    -ms-transition: .25s;
    -o-transition: .25s;
    &:hover{
      opacity: 1;
      width: 250px;
      background-color: #a32c2c;
    }
    img{
      float: right;
    }
  }
  .become{
    top: 120px !important;
  }
  .customize{
    top: 210px !important;
  }
}
.banner { position: relative; overflow: auto; height: auto !important;
  width: 1200px !important;
  margin: 0 auto;
  ul{
    padding: 0;
    margin: 0;
    height: auto !important;
    li { list-style: none; 
      float: left; 
      height: 50%;
      -webkit-transition: .15s;
      -moz-transition: .15s;
      -ms-transition: .15s;
      -o-transition: .15s;
      img{
        width: 100%;
        height: inherit;
      }
    }
  }
  .dots{
    position: absolute;
    z-index: 10;
    bottom: 0;
    padding: 0;
    width: 100%;
    text-align: center;
    li{
      width: 10px;
      height: 10px;
      display: inline-block;
      border-radius: 50%;
      background: white;
      margin: 0 10px;
      color: transparent;
      cursor: pointer;
      opacity: .75;
      -webkit-transition: .25s;
      -moz-transition: .25s;
      -ms-transition: .25s;
      -o-transition: .25s;
      &:hover{
        -webkit-transform: scale(2);
        -moz-transform: scale(2);
        -ms-transform: scale(2);
        -o-transform: scale(2);
      }
      &.active{
        -webkit-transform: scale(2);
        -moz-transform: scale(2);
        -ms-transform: scale(2);
        -o-transform: scale(2);
        opacity: 1;
        background: #bd3a25;
      }
    }
  }
}
.social{
  display: block;
  position: relative;
  text-align: center;
  padding: 20px 0;
  border-bottom: 1px solid rgba(128,128,128,0.46);
  box-shadow: 0 -7px 20px rgba(0, 0, 0, 0.25);
  z-index: 10;
  .fb, .tw, .gg, .yt{
    display: inline-block;
    margin: 0 auto;
    text-transform: center;
    width: 50px;
    height: 50px;
    margin-right: 10px;
    border-radius: 5px;
    background: url(https://cdn.shopify.com/s/files/1/0677/1369/files/Social-media-logos.jpg?6390);
    background-size: 425%;
    -webkit-transition: .15s;
    -moz-transition: .15s;
    -ms-transition: .15s;
    -o-transition: .15s;
    &:hover{
      -webkit-transform: scale(1.25);
      -moz-transform: scale(1.25);
      -ms-transform: scale(1.25);
      -o-transform: scale(1.25);
    }
  }
  .fb{
    background-position: -56px -9px;
  }
  .tw{
    background-position: -3px -9px;
  }
  .gg{
    background-position: -106px -66px;
  }
  .yt{
    background-position: -160px -9px;
  }
}

.articlas{
  a{
    display: inline-block;
    width: 100%;
    height: 160px;
    background-color: blue;
    overflow: hidden;
    .black{
      width: 65%;
      left: -50px;
      display: inline-block;
      background: rgba(0, 0, 0, 0.75);
