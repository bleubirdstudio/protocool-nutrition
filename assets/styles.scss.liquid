          width: 350px;
          height: 85px;
          background: url("https://cdn.shopify.com/s/files/1/0677/1369/files/tag.png?11338");
          background-size: 100%;
          display: block;
          position: absolute;
          top: 300px;
          left: 22%;
        }
        .prod{
          padding: 10px 15px;
          background: rgba(0,0,0,.5);
          border: 3px solid white; 
          position: absolute;
          top: 425px;
          left: 25%;
          display: inline-block;
        }
      }
      .two{
        display: none !important;
      }
      .three{
        display: none !important;
      }
    }
    &:nth-of-type(2){
      .one{
        display: none !important;
      }
      .two{
        display: inline-block;
      }
      .three{
        display: none !important;
      }
    }
    &:nth-of-type(3){
      .one{
        display: none !important;
      }
      .two{
        display: none !important;
      }
      .three{
        display: inline-block;
      }
    }
  }
