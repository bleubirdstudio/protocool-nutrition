@-webkit-keyframes banner {
  0% {-webkit-transform: translateX(1000px);} 
  10% {-webkit-transform: translateX(1050px);} 
  80% {-webkit-transform: translateX(-100px);}  
  100% {-webkit-transform: translateX(0);}
}
@-moz-keyframes banner {
  0% {-moz-transform: translateX(1000px);}  
  100% {-moz-transform: translateX(0);}
}
@-ms-keyframes banner {
  0% {-ms-transform: translateX(1000px);}  
  100% {-ms-transform: translateX(0);}
}
@-o-keyframes banner {
  0% {-o-transform: translateX(1000px);}  
  100% {-o-transform: translateX(0);}
}
  